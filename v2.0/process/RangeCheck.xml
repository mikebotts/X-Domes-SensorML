<?xml version="1.0" encoding="UTF-8"?>
<sml:SimpleProcess gml:id="RangeCheck" xml:lang="en"
    definition="http://sensorml.com/def/sensor/processingSystem"
    xmlns:sml="http://www.opengis.net/sensorml/2.0" 
    xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:gml="http://www.opengis.net/gml/3.2" 
    xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:gco="http://www.isotc211.org/2005/gco"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:ism="urn:us:gov:ic:ism"
    xsi:schemaLocation="http://www.opengis.net/sensorml/2.0 http://schemas.opengis.net/sensorML/2.0/aggregate_process.xsd">
    <gml:identifier codeSpace="uniqueID">urn:q2o:process:RangeCheck</gml:identifier>
    <sml:inputs>
        <sml:InputList>
            <sml:input name="value">
                <swe:Quantity>
                    <swe:uom xlink:href="http://mmisw.org/ont/q2o/parameter/anyUnit"/>
                </swe:Quantity>                           
            </sml:input>
        </sml:InputList>
    </sml:inputs>
    <sml:outputs>
        <sml:OutputList>
            <sml:output name="pass">
                <swe:Boolean/>
            </sml:output>
        </sml:OutputList>
    </sml:outputs>
    <sml:parameters>
        <sml:ParameterList>
            <sml:parameter name="minimum">
                <swe:Quantity>
                    <swe:uom xlink:href="http://mmisw.org/ont/q2o/parameter/anyUnit"/>
                </swe:Quantity>                           
            </sml:parameter>
            <sml:parameter name="maximum">
                <swe:Quantity>
                    <swe:uom xlink:href="http://mmisw.org/ont/q2o/parameter/anyUnit"/>
                </swe:Quantity>
            </sml:parameter>
        </sml:ParameterList>
    </sml:parameters>
    <sml:method xlink:href="urn:org:def:process:Q2O:RangeCheck:1.0"/>
</sml:SimpleProcess>